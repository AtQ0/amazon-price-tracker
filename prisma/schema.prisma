generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id                   Int      @id @default(autoincrement())
  userEmail            String
  amazonId             String
  title                String
  img                  String
  price                Int
  reviewsCount         Int
  reviewsAverageRating Int
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt


}

model ProductDataHistory {
  id                   Int      @id @default(autoincrement())
  amazonId             String
  title                String
  img                  String
  price                Int
  reviewsCount         Int
  reviewsAverageRating Int
  createdAt            DateTime @default(now())
}
